{
  "version": 2,
  "name": "ibcol-micro-graphql-api-production",
  "alias": [
    "api.ibcol.org"
  ],
  "regions": ["iad1"],
  "env": {
    "ENV": "production",
    "NODE_ENV": "production",
    "MONGO_URL": "@ibcol-mongo-url-production"
  },
  "builds": [{ "src": "index.js", "use": "@now/node-server" }],
  "routes": [{ "src": "/.*", "dest": "index.js" }]
}